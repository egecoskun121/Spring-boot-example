type Player{
    id: ID!
    name: String,
    lastName: String,
    position: String

}
input Playerdto{
    name:String!,
    lastName:String,
    position:String
}

type Query{
    getPlayersByPosition(position: String):[Player]
    getById(id: ID):Player
    getPlayerByName(name: String):Player
    getPlayersAll : [Player]

}
type Mutation{
    createPlayer(player: Playerdto):Player
    deletePlayer(player: Playerdto):Player

}